#!/bin/zsh

ssh() {

if ([[$@ = 'LinuxVM']]); then
    echo "Starting Linux VM"
    command ssh gerardo@172.22.222.40

else
    echo $@
    echo "WRONG"
    #command ssh $@

fi
#################################################################################
}
   
matlabRun() {
    matlab -nodisplay -nosplash -nodesktop -r
    "try, run('$1');, catch, exit(1), end, exit(0)";
    echo -e "--- 1 [error] --- 0 [complete]\n Matlab Error Code: ($?)";
}


